    "def smallest_number(list1):\n",
    "    smallest=list1[0]\n",
    "    for i in list1:\n",
    "        if i < smallest:\n",
    "            smallest=i\n",
    "    return smallest\n",
    "\n",
    "list1=[5,7,9,14,10,20,4]\n",
    "smallest=smallest_number(list1)\n",
    "print(\"The smallest number is :\",smallest)\n",
    "\n"
   ]
